<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Prenom</th>
      <th>Code Postal</th>
      <th>Bureau de Vote</th>
  <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.forname %></td>
        <td><%= user.postal %></td>
        <td><%= user.bureau %></td>
        <td><%= link_to 'Show', admin_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

